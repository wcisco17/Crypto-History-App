{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue.js",
  "extends": "./base.json",
  "compilerOptions": {
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "experimentalDecorators": true,
      "strict": true,
      "moduleResolution": "node",
      "jsx": "preserve"
    }
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
